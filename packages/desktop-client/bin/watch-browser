#!/bin/sh -e

ROOT=`dirname $0`
cd "$ROOT/.."

VERSION=`cat "$ROOT/../../desktop-client/package.json" | grep version | head -n1 | awk -F "\"" '{print $4}' | tr -d '\r\n'`

export IS_GENERIC_BROWSER=1
export PORT=3001
export REACT_APP_BACKEND_WORKER_HASH="dev"
export REACT_APP_ACTUAL_VERSION="$VERSION"

yarn start
