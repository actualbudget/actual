  "dependencies": {
    "@actual-app/crdt": "2.1.0",
    "@actual-app/web": "workspace:*",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.9.1",
    "body-parser": "^1.20.3",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "debug": "^4.4.0",
    "express": "4.21.2",
    "express-actuator": "1.8.4",
