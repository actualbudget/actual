    "@types/babel__core": "^7",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/convict": "^6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-actuator": "^1.8.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "http-proxy-middleware": "^3.0.3",
